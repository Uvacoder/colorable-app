{
  "version": 2,
  "name": "colorable",
  "alias": ["colorable.jxnblk.com", "colorable.now.sh"],
  "scope": "jxnblk",
  "builds": [
    {
      "src": "index.js",
      "use": "@now/node"
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "s-maxage=2419200"
        }
      ]
    }
  ]
}
